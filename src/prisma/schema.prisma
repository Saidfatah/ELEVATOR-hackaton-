// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}



model Player {
  id         Int     @id @default(autoincrement())
  firstname  String
  lastname   String
  goal       Int
  salary     Int
  devise     String
  pictureURL String
}